// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model User {
  id   Int     @id @default(autoincrement())
  name String?
}

model Item {
    id Int     @id @default(autoincrement())
    name String?
    image_url String?
    description String?
}
